<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.niruta.mycontactlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContactActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContactListActivity"
            android:label="@string/title_activity_contact_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ContactMapActivity"
            android:label="@string/title_activity_contact_map"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ContactSettingsActivity"
            android:label="@string/title_activity_contact_settings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DatePickerDialog"
            android:label="@string/title_activity_date_picker_dialog"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
